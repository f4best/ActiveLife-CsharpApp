<Window x:Class="programowanieIVprojekt.AdminPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:programowanieIVprojekt"
        mc:Ignorable="d"
        Title="ActiveLife" Height="510" Width="400" Icon="/ico.png"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0.52">
                <GradientStop Color="#FFDAFF00"/>
                <GradientStop Color="#FF717D00" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Dodaj produkt" MinWidth="100" Click="admPanel_Click1"/>
            <Button Content="Usuń produkt" MinWidth="100" Margin="10,0,0,0" Click="admPanel_Click2"/>
        </StackPanel>
        <Frame x:Name="Main" Margin="0,35,0,0" />
    </Grid>
</Window>
